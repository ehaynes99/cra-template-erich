    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.0.9",
    "lint-staged": "^9.5.0",
    "mutationobserver-shim": "^0.3.3",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
